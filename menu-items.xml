<?xml version="1.0" encoding="UTF-8"?>
<atv>
  <body>
    <listWithPreview id="com.sample.menu-items">
      <header>
        <simpleHeader accessibilityLabel="Menu items">
          <title>Menu Items</title>
        </simpleHeader>
      </header>
      <preview>
        <crossFadePreview>
          <image>http://sample-web-server/sample-xml/images/tv1.png</image>
        </crossFadePreview>
      </preview>
      <menu>
        <sections>
          <menuSection>
            <items>
              <oneLineMenuItem accessibilityLabel="List item 1" id="list_1">
                <label>Menu Item Title</label>
                <stash>
                  <data>Store data in an items stash that is specific to the item</data>
                </stash>
              </oneLineMenuItem>
              <oneLineMenuItem accessibilityLabel="List item 2" id="list_2">
                <label>With Right Label</label>
                <rightLabel>Right Label</rightLabel>
              </oneLineMenuItem>
              <oneLineMenuItem accessibilityLabel="List item 3" id="list_3">
                <label>Arrow</label>
                <accessories>
                  <arrow/>
                </accessories>
              </oneLineMenuItem>
              <oneLineMenuItem accessibilityLabel="List item 4" id="list_4">
                <label>Shuffle</label>
                <accessories>
                  <shuffle/>
                </accessories>
              </oneLineMenuItem>
              <oneLineMenuItem accessibilityLabel="List item 5" id="list_5">
                <label>Refresh</label>
                <accessories>
                  <refresh/>
                </accessories>
              </oneLineMenuItem>
              <oneLineMenuItem accessibilityLabel="List item 6" id="list_6">
                <label>Paired</label>
                <accessories>
                  <paired/>
                </accessories>
              </oneLineMenuItem>
              <oneLineMenuItem accessibilityLabel="List item 7" id="list_7">
                <label>Lock</label>
                <accessories>
                  <lock/>
                </accessories>
              </oneLineMenuItem>
              <oneLineMenuItem accessibilityLabel="List item 8" id="list_8">
                <label>Spinner</label>
                <accessories>
                  <spinner/>
                </accessories>
              </oneLineMenuItem>
              <oneLineMenuItem accessibilityLabel="List item 9" id="list_9">
                <label>Download</label>
                <accessories>
                  <download/>
                </accessories>
              </oneLineMenuItem>
              <oneLineMenuItem accessibilityLabel="List item 10" id="list_10">
                <label>Music Video</label>
                <accessories>
                  <musicVideo/>
                </accessories>
              </oneLineMenuItem>
              <oneLineMenuItem accessibilityLabel="List item 11" id="list_11">
                <label>Cloud</label>
                <accessories>
                  <cloud/>
                </accessories>
              </oneLineMenuItem>
              <oneLineMenuItem accessibilityLabel="List item 12" id="list_12">
                <label>Partially Played Dot</label>
                <accessories>
                  <partiallyPlayedDot/>
                </accessories>
              </oneLineMenuItem>
              <oneLineMenuItem accessibilityLabel="List item 13" id="list_13">
                <label>BlueDot: Unplayed Dot</label>
                <accessories>
                  <unplayedDot/>
                </accessories>
                <image>http://sample-web-server/sample-xml/images/tv1.png</image>
              </oneLineMenuItem>
              <oneLineMenuItem accessibilityLabel="List item 14" id="list_14">
                <label>Check Mark</label>
                <accessories>
                  <checkMark/>
                </accessories>
              </oneLineMenuItem>
              <twoLineMenuItem accessibilityLabel="List item 15" id="list_15">
                <label>Two Line Menu Item</label>
                <label2>Subtext</label2>
                <image>http://sample-web-server/sample-xml/images/tv1.png</image>
                <stash>
                  <data>Store data in an items stash that is specific to the item</data>
                </stash>
              </twoLineMenuItem>
              <twoLineMenuItem accessibilityLabel="List item 15" id="list_15">
                <label>BlueDot: Two Line Menu Item</label>
                <label2>Subtext</label2>
                <accessories>
                  <partiallyPlayedDot/>
                </accessories>
                <image>http://sample-web-server/sample-xml/images/tv1.png</image>
              </twoLineMenuItem>
              <twoLineMenuItem accessibilityLabel="List item 15" id="list_15">
                <label>No Image: Two Line Menu Item</label>
                <label2>Subtext</label2>
              </twoLineMenuItem>
              <twoLineMenuItem accessibilityLabel="List item 15" id="list_15">
