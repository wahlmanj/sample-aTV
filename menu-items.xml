                <label2>Subtext</label2>
                <accessories>
                  <partiallyPlayedDot/>
                </accessories>
              </twoLineMenuItem>
              <twoLineEnhancedMenuItem accessibilityLabel="List item 16" id="list_16">
                <label>Two Line Enhanced Menu Item</label>
                <label2>Subtext</label2>
                <rightLabel>2/14</rightLabel>
                <ordinal>2</ordinal>
                <maxOrdinalDigits>1</maxOrdinalDigits>
                <image>http://sample-web-server/sample-xml/images/16x9.png</image>
                <defaultImage>resource://16x9.png</defaultImage>
                <stash>
                  <data>Store data in an items stash that is specific to the item</data>
                </stash>
              </twoLineEnhancedMenuItem>
              <twoLineEnhancedMenuItem accessibilityLabel="List item 16" dimmed="true" id="list_16">
                <label>BlueDot: Two Line Enhanced Menu Item</label>
                <label2>Subtext</label2>
                <rightLabel>2/14</rightLabel>
                <ordinal>2</ordinal>
                <maxOrdinalDigits>1</maxOrdinalDigits>
                <accessories>
                  <partiallyPlayedDot/>
                </accessories>
                <image>http://sample-web-server/sample-xml/images/16x9.png</image>
                <defaultImage>resource://16x9.png</defaultImage>
              </twoLineEnhancedMenuItem>
              <posterMenuItem accessibilityLabel="List item 17" id="list_17">
                <label>Poster Menu Item</label>
                <label2>Subtext</label2>
                <image>http://sample-web-server/sample-xml/images/movie1.png</image>
                <stash>
                  <data>Store data in an items stash that is specific to the item</data>
                </stash>
              </posterMenuItem>
              <posterMenuItem accessibilityLabel="List item 17" id="list_17">
                <label>BlueDot: Poster Menu Item</label>
                <label2>Subtext</label2>
                <accessories>
                  <unplayedDot/>
                </accessories>
                <image>http://sample-web-server/sample-xml/images/movie1.png</image>
              </posterMenuItem>
              <imageTextImageMenuItem accessibilityLabel="Army at navy" dimmed="false" id="list_18">
                <leftImage>http://sample-web-server/sample-xml/images/team1-98x98.png</leftImage>
                <imageSeparatorText>vs</imageSeparatorText>
                <rightImage>http://sample-web-server/sample-xml/images/team2-98x98.png</rightImage>
                <label>ARM @ NAV</label>
                <rightLabel>8:00 PM ET</rightLabel>
                <stash>
                  <data>Store data in an items stash that is specific to the item</data>
                </stash>
              </imageTextImageMenuItem>
              <comboMenuItem id="item_3">
                <image>http://17.201.139.193/sample-xml/images/thumbnail-tv3.jpg</image>
                <label>A basket full of puppies</label>
                <label2>imontheinternet</label2>
                <bottom-left-label>10:20</bottom-left-label>
                <bottom-center-label>102121323232121 views</bottom-center-label>
                <likeDislikeCounts>
                  <likeCount>0</likeCount>
                  <dislikeCount>99</dislikeCount>
                </likeDislikeCounts>
              </comboMenuItem>
              <signInSignOutMenuItem accessibilityLabel="Sign out" id="list_19" signOutExitsApp="true">
                <confirmation>
                  <title>Are You Sure?</title>
                  <description>The account %@ will be logged out of the service.</description>
                </confirmation>
                <stash>
                  <data>Store data in an items stash that is specific to the item</data>
                </stash>
              </signInSignOutMenuItem>
            </items>
          </menuSection>
        </sections>
      </menu>
    </listWithPreview>
  </body>
</atv>